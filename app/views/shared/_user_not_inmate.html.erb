<div class="user">
  <div class="friend_button">
  </div>
  <div class="pic">
    <%= link_to user_path(user) do %>
      <%= image_tag("#{invite_raster_picture(user)}", class: "img_circle") %>
    <% end %>
  </div>
  <div class="friend_name">
    <%= link_to user_path(user) do %>
      <%= user.first_name_with_last_initial %>
    <% end %>
  </div>
</div>